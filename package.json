{
  "name": "@zurdai/n8n-nodes-unlimitedpdf",
  "version": "0.9.0",
  "description": "n8n custom node: convert any input (HTML, text, table, JSON, lists) into a PDF. Free and unlimited!",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json && copy src\\pdf.svg dist\\pdf.svg",
    "test": "jest --runInBand",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "node",
    "pdf",
    "convert",
    "html-to-pdf"
  ],
  "author": "ZurdAI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zurd/n8n_nodes_toPDF_unlimited"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/ConvertToPdf.node.js"
    ]
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "form-data": "^4.0.0",
    "pdf-lib": "^1.17.1",
    "puppeteer": "^24.31.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^24.10.1",
    "html-escaper": "^3.0.3",
    "jest": "^29.6.0",
    "jsdom": "^27.2.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.6"
  }
}
